{
  "name": "@geckoverse/ui",
  "version": "0.1.0",
  "description": "Shared UI components for the Geckoverse (Golden Gecko, Silver Gecko, TeknoGecko)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./gecko": {
      "types": "./dist/gecko/index.d.ts",
      "import": "./dist/gecko/index.mjs",
      "require": "./dist/gecko/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "import": "./dist/auth/index.mjs",
      "require": "./dist/auth/index.js"
    },
    "./settings": {
      "types": "./dist/settings/index.d.ts",
      "import": "./dist/settings/index.mjs",
      "require": "./dist/settings/index.js"
    },
    "./marketing": {
      "types": "./dist/marketing/index.d.ts",
      "import": "./dist/marketing/index.mjs",
      "require": "./dist/marketing/index.js"
    },
    "./seo": {
      "types": "./dist/seo/index.d.ts",
      "import": "./dist/seo/index.mjs",
      "require": "./dist/seo/index.js"
    },
    "./dashboard": {
      "types": "./dist/dashboard/index.d.ts",
      "import": "./dist/dashboard/index.mjs",
      "require": "./dist/dashboard/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "next": "^14 || ^15 || ^16",
    "next-themes": ">=0.3"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "next-themes": {
      "optional": true
    }
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "next": "^15.0.0",
    "next-themes": "^0.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "gecko",
    "ui",
    "components",
    "react",
    "goldengecko",
    "silvergecko"
  ],
  "author": "mariluukkainen",
  "license": "MIT"
}
